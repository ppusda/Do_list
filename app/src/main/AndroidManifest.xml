<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kotlin_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kotlin_App">
        <activity
            android:name=".AppInfoActivity"
            android:theme="@style/popupTheme" />
        <activity android:name=".FinishListActivity" />
        <activity
            android:name=".ChkfinishActivity"
            android:theme="@style/popupTheme" />
        <activity
            android:name=".ChkdeleteActivity"
            android:theme="@style/popupTheme" />
        <activity
            android:name=".EditPopupActivity"
            android:theme="@style/popupTheme" />
        <activity
            android:name=".SelectPopupActivity"
            android:theme="@style/popupTheme" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>